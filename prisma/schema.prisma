
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}


model User {
  id      Int     @id @default(autoincrement())
  email       String @unique
  password    String
  name        String? // se colocar interrogacao siginfica que o campo Ã© opcional
  age         String?
}


